{
  "name": "piecewise",
  "version": "2.0.0",
  "description": "A dynamic standalone webapp for submitting a form with M-Lab test data.",
  "license": "MIT",
  "contributors": [
    "Josh King <josh@throneless.tech",
    "Rae Gaines <rae@throneless.tech"
  ],
  "keywords": [
    "form",
    "webform"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlab/piecewise.git"
  },
  "bugs": {
    "url": "https://github.com/mlab/piecewise.git"
  },
  "directories": {
    "doc": "docs"
  },
  "homepage": "https://github.com/mlab/piecewise#readme",
  "main": "./dist/backend/index.js",
  "bin": {
    "piecewise": "./dist/backend/index.js"
  },
  "files": [
    "/dist"
  ],
  "externals": [
    "pg-native"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix ./src",
    "test": "jest --coverage --forceExit",
    "doc": "jsdoc -d ./dist/doc/ -r ./src/backend ./src/frontend ./src/worker",
    "build:imported-components": "imported-components src/frontend src/frontend/imported.js",
    "build:backend": "cross-env BABEL_ENV=server parcel build src/backend/index.js --dist-dir dist/backend --public-url /",
    "build:frontend": "cross-env BABEL_ENV=client parcel build src/frontend/index.html --dist-dir dist/frontend --target brower --public-url /static",
    "build": "rimraf dist && npm run build:backend && npm run build:imported-components && npm run build:frontend && rimraf dist/imported.js",
    "build:backend:dev": "cross-env BABEL_ENV=server parcel build src/backend/index.js --dist-dir dist/backend --public-url / --target main --no-minify",
    "build:frontend:dev": "cross-env BABEL_ENV=client parcel build src/frontend/index.html src/frontend/imported.js --dist-dir dist/frontend --target browser --no-minify",
    "build:dev": "concurrently -p \"[{name}]\" -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run build:backend:dev\" \"npm run build:frontend:dev\"",
    "start:backend": "node dist/backend",
    "start": "npm run start:backend",
    "start:dev": "nodemon -e js,jsx,html --ignore dist --ignore src/frontend/imported.js --exec 'npm run build:dev && npm run start'"
  },
  "engines": {
    "node": ">=12.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@hapi/joi": "^17.1.1",
    "@koa/cors": "2.2.3",
    "@koa/router": "^8.0.6",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.10",
    "commander": "^4.1.1",
    "date-fns": "^2.12.0",
    "dotenv": "8.0.0",
    "figlet": "^1.2.4",
    "formeo": "^1.4.0",
    "geojson": "^0.5.0",
    "http-errors": "^1.7.3",
    "jwks-rsa": "^1.8.0",
    "knex": "^0.21.1",
    "knex-migrate": "^1.7.4",
    "koa": "2.7.0",
    "koa-404-handler": "0.0.2",
    "koa-better-error-handler": "^3.0.5",
    "koa-better-flash": "0.0.4",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-http-request": "^2.2.1",
    "koa-jwt": "^3.6.0",
    "koa-log4": "^2.3.2",
    "koa-mount": "^4.0.0",
    "koa-parcel-middleware": "^1.0.3",
    "koa-passport": "^4.1.3",
    "koa-session": "^5.13.1",
    "koa-static": "^5.0.0",
    "log4js": "^6.2.0",
    "mapbox-gl": "^1.6.0",
    "material-ui-cookie-consent": "^0.1.0",
    "minimist": "^1.2.5",
    "moment": "^2.25.3",
    "multistream": "^4.0.0",
    "node-fetch": "^2.6.0",
    "papaparse": "^5.2.0",
    "parcel": "^2.0.0-alpha.3.2",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "pg-range": "^1.1.0",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.5",
    "react-imported-component": "^6.2.1",
    "react-prerendered-component": "^1.2.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.1.1",
    "request-promise-native": "^1.0.8",
    "sqlite3": "^4.1.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "acorn-jsx": "^5.2.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.2.1",
    "csv-loader": "^3.0.3",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "2.3.0",
    "jest": "^25.3.0",
    "jsdoc": "^3.6.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.4.0",
    "parcel-plugin-shebang": "^1.2.8",
    "pg-native": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "1.17.1",
    "react-hot-loader": "^4.12.20",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "tcp-port-used": "^1.0.1",
    "xml-loader": "^1.2.1"
  },
  "targets": {
    "main": {
      "publicUrl": "/",
      "context": "node",
      "engines": {
        "node": ">=12.0"
      }
    },
    "browser": {
      "publicUrl": "/static",
      "context": "browser",
      "engines": {
        "browsers": [
          "> 2%",
          "not dead"
        ]
      }
    }
  },
  "babel": {
    "plugins": [
      [
        "react-imported-component/babel"
      ]
    ],
    "exclude": [
      "/mapbox-gl/"
    ],
    "ignore": [
      "./src/frontend/data"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env"
          ],
          [
            "@babel/preset-react",
            {
              "targets": {
                "browsers": "last 2 Firefox versions, last 2 Chrome versions, last 2 Edge versions, last 2 Safari versions"
              }
            }
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-syntax-dynamic-import"
          ]
        ]
      },
      "server": {
        "plugins": [
          "react-imported-component/babel",
          "@babel/plugin-syntax-dynamic-import",
          [
            "transform-inline-environment-variables",
            {
              "include": [
                "npm_package_name",
                "npm_package_version",
                "npm_package_description"
              ]
            }
          ]
        ]
      },
      "client": {
        "presets": [
          [
            "@babel/preset-env"
          ],
          [
            "@babel/preset-react",
            {
              "targets": {
                "browsers": "last 2 Firefox versions, last 2 Chrome versions, last 2 Edge versions, last 2 Safari versions"
              }
            }
          ]
        ],
        "plugins": [
          [
            "react-imported-component/babel"
          ]
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./.jest/env.js"
    ],
    "modulePathIgnorePatterns": [
      "./src/frontend/data"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "proseWrap": "always"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jest/recommended",
      "plugin:node/recommended-module",
      "plugin:prettier/recommended",
      "plugin:promise/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es2017": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "node/shebang": [
        "error",
        {
          "convertPath": {
            "src/**/*.js": [
              "^src/(.+?)\\.js$",
              "dist/$1.js"
            ]
          }
        }
      ]
    }
  }
}
